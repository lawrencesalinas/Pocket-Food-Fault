<h2>Restaurants around <%= zipcode %></h2>
    <% if(results == undefined) {%>
            <h3>results not found</h3>
    <% } %>
<ul>
    <%results.forEach(restaurant => { %>
        <div id = "results-page">
                <a class = login-out href="/restaurants/<%=restaurant.restaurant_id%>">
                <%=restaurant.restaurant_name%><br> cuisine(s): 
                <% restaurant.cuisines.forEach(cuisine=> { %>
                    <% if(cuisine == ''){   %>
                            n/a  
                    <% } else {  %>
                    <small><%=cuisine%></small>,
                     <% } %>
                <%  }) %>
            </a>
        </div>
    <% })%>
</ul>

<form action = "/">
    <input type = "submit" class="btn btn-danger" value = "back to home"">
  </form>